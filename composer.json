{
  "name": "netlogix/nxsolrajax",
  "type": "typo3-cms-extension",
  "description": "TYPO3 Extension \"nxsolrajax\"",
  "license": "MIT",
  "require": {
    "php": "^7.4 || ^8.0",
    "apache-solr-for-typo3/solr": "^11.5",
    "netlogix/nxajaxpluginpage": "^2.1.0",
    "typo3/cms-core": "^11.5",
    "ext-json": "*"
  },
  "require-dev": {
    "ext-sqlite3": "*",
    "nimut/testing-framework": "^6.0",
    "nimut/phpunit-merger": "^1.1",
    "phpunit/phpcov": "^8.2",
    "brianium/paratest": "^6.6"
  },
  "autoload": {
    "psr-4": {
      "Netlogix\\Nxsolrajax\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Netlogix\\Nxsolrajax\\Tests\\": "Tests/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "nxsolrajax",
      "web-dir": ".Build/Web",
      "cms-package-dir": "{$vendor-dir}/typo3/cms"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin",
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@prepare-extension-test-structure",
      "mkdir --parents .Build/logs/"
    ],
    "prepare-extension-test-structure": [
      "Nimut\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
    ]
  }
}
